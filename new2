travels = ['Japan','Europe','America','India','China','Singapore','Thailland','Korea']
print('I Love this travels places :')
print(travels)
print('This is sorted list of travels places :')
print(sorted(travels))
print(travels)
print(sorted(travels,reverse=True))
print(travels)
travels.reverse()
print('The 1st reverse :')
print(travels)
travels.reverse()
print('The 2nd reverse :')
print(travels)
travels.sort()
print('The 1st sort :')
print(travels)
travels.sort(reverse=True)
print('The 2nd sort :')
print(travels)
print("This is totle: {} Places that I Love it".format(len(travels)))